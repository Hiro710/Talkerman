<div class="container">
  <h1>Chat Room</h1>

  <form class="form-inline">
    <div class="form-group">
      <input type="text" class="form_control" id="chat-input"/>
    </div>
    <button class="btn btn-primary" id="button">送信</button>
  </form>

  <div id="messages">
    <ul class="list-group">
      <%= render @messages %>
    </ul>
  </div>
</div>
